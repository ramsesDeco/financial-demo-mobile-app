{
  "name": "financial-demo-mobile-app",
  "version": "0.0.1",
  "description": "App to apply knowledge about react-native, graphql, apollo-client and some other UX plugins",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "hook:commit-msg": "commitlint -e",
    "hook:pre-commit": "lint-staged",
    "ios": "react-native run-ios",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "postinstall": "husky install",
    "start": "react-native start",
    "test": "jest"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "preset": "react-native"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "dependencies": {
    "react": "17.0.1",
    "react-native": "0.64.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@commitlint/cli": "12.1.1",
    "@commitlint/config-conventional": "12.1.1",
    "@commitlint/prompt-cli": "12.1.1",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/jest": "^26.0.20",
    "@types/react-native": "^0.64.0",
    "@types/react-test-renderer": "^16.9.2",
    "babel-eslint": "10.1.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.14.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "7.23.1",
    "format-package": "6.1.0",
    "husky": "6.0.0",
    "jest": "^26.6.3",
    "lint-staged": "10.5.4",
    "metro-react-native-babel-preset": "^0.64.0",
    "prettier": "2.2.1",
    "react-test-renderer": "17.0.1",
    "typescript": "^3.8.3"
  }
}
